    .ad_content{
        width:990px;
        margin:0 auto;
        position:relative;
        top:0;
        z-index:100000;
    }
    .yundong_ad_spic{
        position:absolute;
        width:74px;
        height:86px;
        bottom:0px;
        right:0px;
        overflow:hidden;
        cursor:pointer;
    }
    .yundong_ad_lpic{
        position:absolute;
        width:74px;
        height:86px;
        bottom:0px;
        right:0px;
        overflow:hidden;
    }
    .yundong_ad_con{
        position:absolute;
        top:0px;
        right:-74px;
        width:74px;
        height:300px;
        overflow:hidden;
        display:none;
    }
    .yundong_ad_close{
        position:absolute;
        top:10px;
        right:10px;
        width:16px;
        height:16px;
        background: #aaa;
        cursor:pointer;
        display:none;
        background: url(http://img04.taobaocdn.com/tps/i4/T1Xdt0XeRvXXXXXXXX-16-16.png) no-repeat;
    }
</style>
<script>
  
    window.onload=function(){
        // if(!D.get(".yundong_ad_con")) return;
        var S=KISSY,
        E=S.Event,
        D=S.DOM,
        showCount=1,   //每展现一次此参数加一,用来在每次展开之后,计时三秒后判断是否还是当前的展开
        adCon=D.get(".yundong_ad_con"),  //容器
        sEle=D.get(".yundong_ad_spic"),   //小图
        lEle=D.get(".yundong_ad_lpic"),  //大图
        close=D.get(".yundong_ad_close")  //关闭按钮
        
        D.css(lEle,"opacity","0");//先隐藏大图
        D.css(adCon,"display","block"); //显示容器,只有在页面完全加载完后才显示
        //拖动和resize的时候调整高度
        function setPos(){
            adCon.style.top=(D.scrollTop()+D.viewportHeight()-406)+"px";
        }
        setPos();//初始化高度
        E.on(window,"scroll",setPos);
        E.on(window,"resize",setPos);
        E.on(close,"click",showSmall)
        E.on(sEle,"click",showLarge)
        //从小变大
        function showLarge(){
            adCon.style.width="1000px";
            S.Anim(lEle,{width:"1000px",height:"300px",opacity:"1"},0.3,undefined,function(){close.style.display="block"}).run();
            S.Anim(sEle,{opacity:"0"},0.3).run();
            var count=++showCount //showCount递增一个,同时记录当前的数值
            setTimeout(function(){
                if(count==showCount){ //比较是否是同一次展开
                    showSmall();
                }
            },3000)
        }
        //从大变小
        function showSmall(){
            close.style.display="none"
            adCon.style.width="1000px";
            S.Anim(lEle,{width:"74px",height:"86px",opacity:"0"},0.3).run();
            S.Anim(sEle,{opacity:"1"},0.6).run();
        }
        showLarge();
    }
       
</script>