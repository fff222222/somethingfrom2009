  "icons": { "48": "icon.png",
          "128": "icon.png" ,"16": "icon.png"},
  "description": "",
  "browser_action": {
    "default_icon": "icon.png"  
  },
  "permissions": [ "tabs","http://*/","https://*/","https://*/*","http://*/*","contextMenus","notifications" ],
