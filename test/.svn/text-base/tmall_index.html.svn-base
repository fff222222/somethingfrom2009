MFP.add('floor', function(M) {

    var S = KISSY, Event = S.Event;

    M.Floor = {
        inited:[],
        init: function(floorId) {
            //防止重复初始化
            if(S.indexOf(floorId,M.Floor.inited) >= 0) return;

            var defaultConfig = {
                    contentCls: 'f-switchable',
                    navCls: 'f-trigger',
                    lazyDataType: 'img-src',
                    autoplay: false,
                    effect: 'scrollx',
                    circular: true
                },
                CLICK = 'click';

			S.query(floorId + ' div.f-carousel').each(function(container) {
				var t = new S.Tabs(container, defaultConfig);

				Event.on(S.get('a.mall-prev', container), CLICK, function(ev) {
					ev.halt();
					t.prev();
				});

				Event.on(S.get('a.mall-next', container), CLICK, function(ev) {
					ev.halt();
					t.next();
				});
                // 随机切换
                t.switchTo(Math.floor(Math.random()*(t.panels.length)));
			});

			S.query(floorId + ' div.f-slider').each(function( container ) {
                var s = S.Slide(container, S.merge(defaultConfig, { effect: 'scrolly' }));

                // 随机切换
                s.switchTo(Math.floor(Math.random()*(s.panels.length)));
			});

			// 楼层logo hover
			function switchImg( evt ) {
				var img = this, src = img.src, DATA = 'data-src';
				try {
					img.src = img.getAttribute( DATA );
					img.setAttribute( DATA, src);
				} catch (e) {}
			}

			S.query(floorId + ' img.J_ShowLogo').each(function( container ) {
				if ( !container.getAttribute('data-ks-event-guid') ) {
					Event.on( container, 'mouseenter', switchImg);
					Event.on( container, 'mouseleave', switchImg);
				}
            });

            //防止重复初始化
            M.Floor.inited.push(floorId);
        }
    };
});
