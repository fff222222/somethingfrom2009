        };

    function MegaMenu(container, config) {
        var self = this;

        // factory or constructor
        if (!(self instanceof MegaMenu)) {
            return new MegaMenu(container, config);
        }
        self.container = S.get(container);
        self.config = S.merge(defaultConfig, config || {});
        self.config.viewer = S.get(config.view);
        self.triggers = Dom.query(config.triggers,self.container);

        self._init();
    }

	function indexOf(a, val) {
		for (var i=0; i<a.length; i=i+1) {
			if (a[i] === val) {
				return i;
			}
		}

		return -1;
	}

var ajax={
        _msxmls:[
            'Microsoft.XMLHTTP',
            'MSXML2.XMLHTTP.3.0',
            'MSXML2.XMLHTTP'
        ],
        init:function(url,success){
            this.timer_timeout=null;//计时器,用来设置超时
            this.timer_state_watch=null;//计时器,用来监视readystate,避开了浏览器兼容性
            this.url=url
            this.obj={}
            this.createXhrObject()
            this.send()

            this.timeout=100
            this.success=success;
            this.fail=function(){

            }
            this.abort=function(){

            }

        },
        /**
         *生成一个XMLHttpRequest对象,
         */
        createXhrObject:function(){
            var obj,i
            try
            {
                // Instantiates XMLHttpRequest in non-IE browsers and assigns to http.
                this.obj  = new XMLHttpRequest();
                //  Object literal with http and tId properties

            }
            catch(e)
            {
                for(i=0; i<this._msxmls.length; ++i){
                    try
                    {
                        // Instantiates XMLHttpRequest for IE and assign to http
                        this.obj  = new ActiveXObject(this._msxmls[i]);
                        break;
                    }
                    catch(e1){}
                }
            }
            finally
            {
                return obj;
            }

        },
        send:function(){
            this.obj.open("GET",this.url, true);
            this.beginStateWatch()
            this.obj.send(null);

        },
        beginStateWatch:function(){
            var o=this.obj;
            var now=this;
            if(this.timeout) this.timer_timeout=setTimeout(function(){
                o.abort();
                clearTimeout(now.timer_timeout)
                now.timer_timeout=null;
                now.onAbort();
            },this.timeout*1000)
            this.timer_state_watch=setInterval((function(param){
                return function(){

                    param.handleReadyState()
                }
            })(this),10);

        },
        /**
         * 这里的处理比较简单,忽略了除了status=200的所有其他情况,任何异常都将导致收不到任何回应
         */
        handleReadyState:function(){

            var o=this.obj;
            if(o.readyState === 4&&o.status==200){
                if(this.timer_timeout) {
                    clearTimeout(this.timer_timeout);
                    this.timer_timeout=null;
                }
                clearInterval(this.timer_state_watch);
                this.timer_state_watch=null;
                this.onSuccess()
            }else if(o.readyState === 4){
                this.onFail()
            }
        },
        onSuccess:function(){
            this.success(this.obj)
        },
        onAbort:function(){
            this.abort(this.obj)
        },
        onFail:function(){
            this.fail(this.obj)
        }
    }
    S.mix(MegaMenu.prototype, {
        switchTo: function(idx) {
            var self = this,
            triggers = self.triggers,
            subViews = self.subViews;

            S.each(triggers,function(trigger){
                Dom.removeClass(trigger, SELECTED);
            })
            Dom.addClass(self.triggers[idx], SELECTED);
            S.each(subViews,function(sub){
                Dom.addClass(sub, HIDDEN);
            })
            Dom.removeClass(subViews[idx], HIDDEN);
        },
        show: function() {
            var self = this;

            self.container.style.zIndex = 20;
            //Dom.addClass( elem, SELECTED );
            Dom.removeClass(self.viewer, HIDDEN);
        },
        hide: function() {
            var self = this,
            triggers = self.triggers;

            self.container.style.zIndex = 15;

            S.each(triggers,function(trigger){
                Dom.removeClass(trigger, SELECTED);
            })
            Dom.addClass(self.viewer, HIDDEN);
        },
        getViewerData: function(url){
            var self = this;
            //request once
            /*
            if(!self.config.getting){
                S.getScript(url + '?t=' + new Date().getTime(),function(){
                    if(Mall&&Mall.SubCat){
                        var frag = document.createElement('div');
                        frag.innerHTML = Mall.SubCat;
                        self.container.appendChild(frag);
                        //late build viewer
                        self.config.viewer = S.get(self.config.view);
                        self._initView();
                        //hidden status? do nothing
                        if('hidden' == self.status) return;
                        //first show
                        self.switchTo(self.config.idx);
                        self.show();
                    }
                })
           }*/
 if(!self.config.getting){
        ajax.init("http://www.tmall.com/go/rgn/test/dsssssssssssssssssssssssssssssssssssssss.php?"+Math.random(),function(o){
		self.config.viewer=o.responseText;
		self._initView();
                        //hidden status? do nothing
                        if('hidden' == self.status) return;
                        //first show
                        self.switchTo(self.config.idx);
                        self.show();
		});
}
            self.config.getting = true;
        },

        _init: function() {
            var self = this, cfg = self.config;

            /*asyn request first
            Event.on( self.container, MOUSEENTER, function() {
                if(!self.config.viewer){
                   self.getViewerData(self.config.dataUrl);
                }
            });
            */

            S.each( self.triggers, function( trigger ) {
                //暂时阻止主类目事件传播
                Event.on( trigger, CLICK, function( evt ){
                    evt.halt();
                    return false;
                });
				Event.on( trigger, MOUSEENTER, function( evt ) {
					evt.halt();
					//var idx = self.triggers.indexOf(this);
					var idx = indexOf( self.triggers, trigger );

                    //for asyn loaded showing
                    self.config.idx = idx;
                    //reset status
                    self.status = 'visible';
                    //init viewer
                    if (!self.viewer) {
                        //if textarea isnt exist,try again
                        if(!self.config.viewer){
                            self.getViewerData(self.config.dataUrl);
                            //do nothing
                            return;
                        }
                        //exist? continue init
						self._initView();
					}

                    //延迟显示
                    if(self.showTimer) clearTimeout(self.showTimer);

                    self.showTimer = setTimeout(function(){
                        if(!self.hideTimer){
                            //先切换
                            self.switchTo(idx);
                            //后显示
                            self.show();
                        }
                    },self.config.hideDelay * 1000);

					self.hideTimer = clearTimeout(self.hideTimer);
				});

				Event.on( trigger, MOUSELEAVE, function( evt ) {
                    //set hidden, forbid getViewerData to show the viewer
                    //when mouse leave from the trigger
                    self.status = 'hidden';
                    if (self.showTimer) clearTimeout(self.showTimer);
					if (self.hideTimer) return;

					self.hideTimer = setTimeout(function() {
						self.hide();
						self.hideTimer = clearTimeout(self.hideTimer);
                    }, self.config.hideDelay * 1000);
				});
			})
        },

        _initView: function() {
            var self = this, cfg = self.config;

            if (!self.viewer) {
                self.viewer = doc.createElement(DIV);
                self.viewer.className = "pop-subcategory hidden"

                self.viewer.innerHTML = cfg.viewer;
                self.container.appendChild(self.viewer);

                // init subView
                self._initSubView();
            }
        },

        _initSubView: function() {
            var self = this, i = 0;

            self.subViews = Dom.query(self.config.subViews, self.container);
            S.each(self.subViews, function( subView ) {
                var triggers = Dom.query('dl.J_HotMenuItem',subView),
                    panels = Dom.query('div.J_HotView',subView),
                    tabs;

                tabs = new S.Tabs(subView, {triggerType: 'mouse', triggers: triggers, panels: panels});

				// init subView
				Event.on( subView, MOUSEENTER, function() {
					self.hideTimer = clearTimeout(self.hideTimer);
					self.selectedSubView = this;
				});

				Event.on( subView, MOUSELEAVE, function() {
					self.hideTimer = setTimeout(function() {
						self.hide();
						self.hideTimer = clearTimeout(self.hideTimer);
					}, self.config.hideDelay * 1000);
				});
            });


        }
    });

    M.Category = MegaMenu;
});
/**
 * 首页顶部滚动广告 v1.2 by yuyin
 * @date:2010-5-19
 * @TODO:去YAHOO用KISSY
 */

(function(){
    var S = KISSY,Dom = S.DOM,Event = S.Event;

    var pere = S.get('#J_Peremptory');

    if(!pere) return;

    var cont = S.get('#J_PereContent'), prt = cont.parentNode, trigger = S.get('#J_PereTrigger');

    var prtHeight = 370, defHeight = 341, defTop = Dom.getY(cont), aimTop = defTop + defHeight, timer = 1000, timerHolder = null, STR_CLOSE = '关闭广告', STR_TITLE = trigger.innerHTML, strCookie = 'dropad', isAnswer = false, isOut = true;

    function anim(ele,x,y){
        return new YAHOO.util.Motion(ele, {points: { to : [x,y] }}, 1, YAHOO.util.Easing.easyIn);
    }

    function completeHandle(){
        //接受鼠标响应
        isAnswer = true;

        //关闭后调整层级
        if('on' != trigger.className ){
            pere.style.zIndex = '11';
            prt.style.height = '24px';
        }

        //动画完毕，鼠标在广告区外，用户没有操作时 延迟3000毫秒
        if(isOut)
            leaveHandle(3);
    }

    function leaveHandle(t){
        //默认延迟500毫秒
        if(t === undefined) t = 0.5;
        //仅在允许响应时开始延迟
        if('on' == trigger.className && isAnswer)
            timerHolder = setTimeout(trig,timer*t);
    }

    function trig(){
        var ht = 'on' == trigger.className ? defTop : aimTop, animcnt = anim(cont,Dom.getX(cont),ht), cache = trigger.className;
        clearTimeout(timerHolder);
        //动画时禁止鼠标响应
        isAnswer = false;
        //显示时层级调高
        pere.style.zIndex = '16';
        prt.style.height = prtHeight + 'px';
        //切换触点内容
        trigger.className = trigger.getAttribute('data-todo');
        trigger.innerHTML = 'on' == trigger.className ? STR_CLOSE : STR_TITLE ;
        trigger.setAttribute('data-todo',cache);
        //动画
        animcnt.onComplete.subscribe(completeHandle);
        animcnt.animate();
    }

    Event.on(trigger,'click',trig);

    Event.on(cont,'mouseleave',function(){
        //移出内容区域
        isOut = true;
        //自主移开，延迟500毫秒，快速关闭
        leaveHandle();
    });

    Event.on(cont,'mouseenter',function(){
        //移入内容区域
        isOut = false;
        //禁止响应
        clearTimeout(timerHolder);
    });

    //初始化容器
    prt.style.height = '24px';

    //仅在当天首次打开浏览器时自动下拉
    if(!S.Cookie.get(strCookie)){
        //偷懒，3秒后内容差不多载入完毕，自动下拉，
        timerHolder = setTimeout(trig,timer*3);

        //记录到cookie
        S.Cookie.set(strCookie,'read',1,document.domain);
    }

})();

