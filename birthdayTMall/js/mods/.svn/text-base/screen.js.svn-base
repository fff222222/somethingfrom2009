define(function(require, exports, module) {
    var util=require("core.js")
    var screen=function(){
        //属性
        var props={
            width:0,
            height:0,
            x:0,
            y:0,
            index:0,
            
        }
    }
    util.ap(screen,{
        get:function(propName){
            return this.props[propName];
        },
        set:function(propName,value){
            this.props[propName]=value;
        }
    })

    return screen;
});

